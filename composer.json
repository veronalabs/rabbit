{
    "name": "veronalabs/rabbit",
    "description": "A modern way of building WordPress plugins.",
    "keywords": [
        "wordpress",
        "pressmodo",
        "sematico",
        "rabbit",
        "framework"
    ],
    "homepage": "https://github.com/veronalabs/rabbit",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/veronalabs/rabbit/issues"
    },
    "autoload": {
        "psr-4": {
            "Rabbit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rabbit\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "caseyamcl/configula": "^3.1",
        "illuminate/database": "^8.61",
        "illuminate/macroable": "^8.7",
        "laminas/laminas-diactoros": "^2.5",
        "laminas/laminas-escaper": "^2.7",
        "laminas/laminas-form": "^2.15",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-view": "^2.11",
        "league/container": "^3.3",
        "symfony/finder": "^5.1",
        "symfony/http-foundation": "^5.1",
		"veronalabs/logger-wp": "^1.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^5.1",
        "wp-coding-standards/wpcs": "dev-master"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": "bin/phpunit"
    },
    "prefer-stable": true
}
